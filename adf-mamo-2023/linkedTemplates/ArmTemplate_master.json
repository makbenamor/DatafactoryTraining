{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-mamo-2023"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"Blob_Storage_bstorageeurostat_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Blob_Storage_bstorageeurostat'"
		},
		"DataLakeStorage_sakimo2024_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DataLakeStorage_sakimo2024'"
		},
		"APIPlayer_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.apilayer.com"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0327-110020-m63kpskf"
		},
		"DataLakeStorage_sakimo2024_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sakimo2024.dfs.core.windows.net/"
		},
		"HTTP_dynamic_lS_ECDC_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().BaseURL}"
		},
		"Http_Ecdc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"weather_gov_API_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.weather.gov"
		},
		"event_trigger_1_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/47f66aa8-8d49-4acd-85a1-3370d33b0508/resourceGroups/rgmamor/providers/Microsoft.Storage/storageAccounts/bstorageeurostat"
		},
		"scheduled1_properties_Ingest_raw_from_HTTP_to_ADLS_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "hospital_admissions.csv"
		},
		"scheduled1_properties_Ingest_raw_from_HTTP_to_ADLS_parameters_sourceRelativeURL": {
			"type": "string",
			"defaultValue": "/cloudboxacademy/covid19/main/raw/main/ecdc_data/hospital_admissions.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-mamo-2023_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"Blob_Storage_bstorageeurostat_connectionString": {
						"value": "[parameters('Blob_Storage_bstorageeurostat_connectionString')]"
					},
					"DataLakeStorage_sakimo2024_accountKey": {
						"value": "[parameters('DataLakeStorage_sakimo2024_accountKey')]"
					},
					"APIPlayer_properties_typeProperties_url": {
						"value": "[parameters('APIPlayer_properties_typeProperties_url')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"DataLakeStorage_sakimo2024_properties_typeProperties_url": {
						"value": "[parameters('DataLakeStorage_sakimo2024_properties_typeProperties_url')]"
					},
					"HTTP_dynamic_lS_ECDC_properties_typeProperties_url": {
						"value": "[parameters('HTTP_dynamic_lS_ECDC_properties_typeProperties_url')]"
					},
					"Http_Ecdc_properties_typeProperties_url": {
						"value": "[parameters('Http_Ecdc_properties_typeProperties_url')]"
					},
					"weather_gov_API_properties_typeProperties_url": {
						"value": "[parameters('weather_gov_API_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-mamo-2023_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-mamo-2023_ArmTemplate_0')]"
			]
		},
		{
			"name": "adf-mamo-2023_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"event_trigger_1_properties_typeProperties_scope": {
						"value": "[parameters('event_trigger_1_properties_typeProperties_scope')]"
					},
					"scheduled1_properties_Ingest_raw_from_HTTP_to_ADLS_parameters_sinkFileName": {
						"value": "[parameters('scheduled1_properties_Ingest_raw_from_HTTP_to_ADLS_parameters_sinkFileName')]"
					},
					"scheduled1_properties_Ingest_raw_from_HTTP_to_ADLS_parameters_sourceRelativeURL": {
						"value": "[parameters('scheduled1_properties_Ingest_raw_from_HTTP_to_ADLS_parameters_sourceRelativeURL')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-mamo-2023_ArmTemplate_1')]"
			]
		}
	]
}